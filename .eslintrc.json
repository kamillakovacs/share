{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "next"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "warn"
    }
}
